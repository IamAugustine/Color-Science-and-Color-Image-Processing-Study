<docking:DockItem x:Class="ColorImageProcessing.Entities.ImageInfoPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ColorImageProcessing.Entities"
      xmlns:docking="http://schemas.devzest.com/presentation/docking"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      DefaultDockPosition="Right"
      Style="{DynamicResource {x:Type docking:DockItem}}"
      Title="ImageInfoPage">
    <Grid>

        <DataGrid ItemsSource="{Binding}" AutoGenerateColumns="False" >
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Hello}" Header="Property" CanUserResize="False" />
                <DataGridTextColumn Binding="{Binding World}" Header="Value" CanUserResize="False" />
            </DataGrid.Columns>
        </DataGrid>
        <!--<local:ImageInfoPage HorizontalAlignment="Left" VerticalAlignment="Top"/>-->

    </Grid>
</docking:DockItem>

